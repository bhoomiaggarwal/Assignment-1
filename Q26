string = "My name is bhoomi"
prefix = input("Enter the prefix: ")
suffix = input("Enter the suffix: ")
if string.startswith(prefix)==True:
    print("The string does start with prefix",prefix)
else:
    print("No it doesnt start with prefix",prefix)

if string.endswith(suffix)==True:
     print("The string does end with suffix",suffix)
else:
    print("No it doesnt end with suffix",suffix)
