nterm = int(input("Enter the number of terms: "))
n1 = 0
n2 = 1
count = 0
if nterm<=0:
    print("Enter a positive number")
elif nterm==1:
    print("Fibonacci sequence:",nterm)
else:
    print("The fibonacci series is: ")
    while count<nterm:
        print(n1)
        n=n1+n2
        n1=n2
        n2=n
        count+=1